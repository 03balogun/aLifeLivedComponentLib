{
  "name": "@alifelived/react-native-component-lib",
  "version": "0.0.9",
  "description": "A component library for a life lived react native",
  "author": {
    "name": "max_ckelly",
    "email": "maxckel@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aLifeLived/aLifeLivedComponentLib"
  },
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "concurrently --kill-others \"npm run storybook\" \"react-native start\"",
    "test": "jest",
    "lint": "eslint .",
    "build": "rollup -c",
    "prestorybook": "rnstl",
    "storybook": "start-storybook -p 7007",
    "build-storybook": "build-storybook"
  },
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@babel/preset-env": "^7.14.8",
    "@babel/preset-react": "^7.14.5",
    "@babel/runtime": "^7.14.8",
    "@react-native-community/eslint-config": "^3.0.0",
    "@rollup/plugin-commonjs": "^19.0.2",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@storybook/addon-actions": "^5.3.21",
    "@storybook/addon-knobs": "^5.3.21",
    "@storybook/addon-links": "^5.3.21",
    "@storybook/addon-ondevice-actions": "^5.3.23",
    "@storybook/addon-ondevice-knobs": "^5.3.25",
    "@storybook/react-native": "^5.3.25",
    "@storybook/react-native-server": "^5.3.23",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/jest-native": "^4.0.1",
    "@testing-library/react-hooks": "^7.0.1",
    "@testing-library/react-native": "^7.2.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^26.0.24",
    "@types/react": "^17.0.15",
    "@types/react-native": "^0.64.12",
    "@typescript-eslint/eslint-plugin": "^4.28.5",
    "@typescript-eslint/parser": "^4.28.5",
    "babel-jest": "^27.0.6",
    "babel-loader": "^8.2.2",
    "concurrently": "^6.2.0",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^6.15.0",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "metro-react-native-babel-preset": "^0.66.2",
    "prettier": "^2.3.2",
    "react": "17.0.1",
    "react-dom": "^17.0.1",
    "react-native": "0.64.2",
    "react-native-storybook-loader": "^2.0.4",
    "react-test-renderer": "17.0.1",
    "rollup": "^2.54.0",
    "rollup-plugin-cleaner": "^1.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-typescript2": "^0.30.0",
    "ts-jest": "^27.0.4",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm test",
      "pre-commit": "npm run lint"
    }
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "./src",
        "./packages"
      ],
      "pattern": "**/*.stories.tsx",
      "outputFile": "./src/storybook/storyLoader.js"
    }
  },
  "dependencies": {
    "@alifelived/react-native-component-lib": "file:alifelived-react-native-component-lib-0.0.9.tgz"
  }
}
